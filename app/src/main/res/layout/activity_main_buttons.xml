<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B0B6B1B1"
    tools:showIn="@layout/app_bar_homeactivity">

    -<androidx.constraintlayout.widget.ConstraintLayout
           android:layout_width="match_parent"
           android:layout_height="match_parent">

           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline11"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               app:layout_constraintGuide_percent="0.5" />

           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline41"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               app:layout_constraintGuide_percent="0.7" />

           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline9"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               app:layout_constraintGuide_percent="0.05" />

           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline8"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="vertical"
               app:layout_constraintGuide_percent="0.28" />

           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline_UserProfile1"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               app:layout_constraintGuide_percent="0.35" />

           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline_UserProfile2"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="vertical"
               app:layout_constraintGuide_percent="0.05" />

           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline_UserProfile3"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="vertical"
               app:layout_constraintGuide_percent="0.73" />

           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline_UserProfile31"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="vertical"
               app:layout_constraintGuide_percent="0.5" />

           <androidx.cardview.widget.CardView
               android:layout_width="0dp"
               android:layout_height="0dp"
               android:layout_marginStart="41dp"
               android:layout_marginLeft="41dp"
               android:layout_marginTop="21dp"
               android:layout_marginEnd="34dp"
               android:layout_marginRight="34dp"
               android:layout_marginBottom="15dp"
               app:layout_constraintBottom_toTopOf="@+id/guideline11"
               app:layout_constraintEnd_toStartOf="@+id/guideline_UserProfile31"
               app:layout_constraintHorizontal_bias="0.0"
               app:layout_constraintStart_toEndOf="@+id/guideline_UserProfile2"
               app:layout_constraintTop_toBottomOf="@+id/guideline9"
               app:layout_constraintVertical_bias="0.0">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/btnReg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="81dp"
        android:layout_marginRight="81dp"
        android:background="@drawable/register_woman"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

               <TextView
                   android:id="@+id/textView4"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_below="@+id/btnReg"
                   android:layout_centerHorizontal="true"
                   android:layout_marginTop="10dp"
                   android:text="REGISTER"
                   android:textSize="20sp"
                   app:layout_constraintEnd_toEndOf="@+id/btnReg"
                   app:layout_constraintHorizontal_bias="0.0"
                   app:layout_constraintStart_toStartOf="@+id/btnReg"
                   app:layout_constraintTop_toBottomOf="@+id/btnReg" />
</androidx.constraintlayout.widget.ConstraintLayout>
           </androidx.cardview.widget.CardView>


           <ImageButton
               android:id="@+id/btnWomen"
               android:layout_width="0dp"
               android:layout_height="0dp"
               android:layout_marginStart="80dp"
               android:layout_marginLeft="80dp"
               android:layout_marginTop="30dp"
               android:layout_marginEnd="80dp"
               android:layout_marginRight="80dp"
               android:layout_marginBottom="30dp"
               android:background="@drawable/women"
               app:layout_constraintBottom_toTopOf="@+id/guideline_UserProfile1"
               app:layout_constraintEnd_toStartOf="@+id/guideline10"
               app:layout_constraintHorizontal_bias="0.0"
               app:layout_constraintStart_toEndOf="@+id/guideline_UserProfile31"
               app:layout_constraintTop_toBottomOf="@+id/guideline9"
               app:layout_constraintVertical_bias="0.0" />

           <TextView
               android:id="@+id/textView5"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_below="@+id/btnWomen"
               android:layout_marginTop="10dp"
               android:text="RECORDS"
               android:textSize="20sp"
               app:layout_constraintEnd_toEndOf="@+id/btnWomen"
               app:layout_constraintHorizontal_bias="0.15"
               app:layout_constraintStart_toStartOf="@+id/btnWomen"
               app:layout_constraintTop_toBottomOf="@+id/btnWomen" />

           <ImageButton
               android:id="@+id/btnReports"
               android:layout_width="0dp"
               android:layout_height="0dp"
               android:layout_centerInParent="true"
               android:layout_marginStart="30dp"
               android:layout_marginLeft="30dp"
               android:layout_marginTop="12dp"
               android:layout_marginEnd="30dp"
               android:layout_marginRight="30dp"
               android:layout_marginBottom="12dp"
               android:background="@drawable/reports_1"
               app:layout_constraintBottom_toTopOf="@+id/guideline41"
               app:layout_constraintEnd_toStartOf="@+id/guideline8"
               app:layout_constraintHorizontal_bias="0.0"
               app:layout_constraintStart_toEndOf="@+id/guideline_UserProfile2"
               app:layout_constraintTop_toBottomOf="@+id/guideline11" />

           <TextView
               android:id="@+id/textView6"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_below="@+id/btnReports"
               android:layout_centerHorizontal="true"
               android:layout_marginTop="10dp"
               android:text="Reports"
               android:textSize="14sp"
               app:layout_constraintEnd_toEndOf="@+id/btnReports"
               app:layout_constraintStart_toStartOf="@+id/btnReports"
               app:layout_constraintTop_toBottomOf="@+id/guideline41" />

           <ImageButton
               android:id="@+id/btnUpdateForms"
               android:layout_width="0dp"
               android:layout_height="0dp"
               android:layout_centerInParent="true"
               android:layout_marginStart="28dp"
               android:layout_marginLeft="28dp"
               android:layout_marginTop="12dp"
               android:layout_marginEnd="28dp"
               android:layout_marginRight="28dp"
               android:layout_marginBottom="12dp"
               android:background="@drawable/update_forms"
               app:layout_constraintBottom_toTopOf="@+id/guideline41"
               app:layout_constraintEnd_toStartOf="@+id/guideline_UserProfile31"
               app:layout_constraintHorizontal_bias="0.0"
               app:layout_constraintStart_toEndOf="@+id/guideline8"
               app:layout_constraintTop_toBottomOf="@+id/guideline11" />

           <TextView
               android:id="@+id/textView2"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_below="@+id/btnUserProfile"
               android:layout_centerHorizontal="true"
               android:layout_marginTop="10dp"
               android:text="Update forms"
               android:textSize="14sp"
               app:layout_constraintEnd_toEndOf="@+id/btnUpdateForms"
               app:layout_constraintHorizontal_bias="0.526"
               app:layout_constraintStart_toStartOf="@+id/btnUpdateForms"
               app:layout_constraintTop_toBottomOf="@+id/guideline41" />

           <androidx.constraintlayout.widget.Guideline
               android:id="@+id/guideline10"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="vertical"
               app:layout_constraintGuide_percent="0.95" />

           <ImageButton
               android:id="@+id/btnLanguage"
               android:layout_width="0dp"
               android:layout_height="0dp"
               android:layout_marginStart="30dp"
               android:layout_marginLeft="30dp"
               android:layout_marginTop="12dp"
               android:layout_marginEnd="30dp"
               android:layout_marginRight="30dp"
               android:layout_marginBottom="12dp"
               android:background="@drawable/language"
               app:layout_constraintBottom_toTopOf="@+id/guideline41"
               app:layout_constraintEnd_toStartOf="@+id/guideline_UserProfile3"
               app:layout_constraintHorizontal_bias="0.0"
               app:layout_constraintStart_toEndOf="@+id/guideline_UserProfile31"
               app:layout_constraintTop_toBottomOf="@+id/guideline11" />

           <TextView
               android:id="@+id/textView3"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_below="@+id/btnUserProfile"
               android:layout_centerHorizontal="true"
               android:layout_marginTop="10dp"
               android:text="Language"
               android:textSize="14sp"
               app:layout_constraintEnd_toEndOf="@+id/btnLanguage"
               app:layout_constraintStart_toStartOf="@+id/btnLanguage"
               app:layout_constraintTop_toBottomOf="@+id/guideline41" />

           <ImageButton
               android:id="@+id/btnUserProfile"
               android:layout_width="0dp"
               android:layout_height="0dp"
               android:layout_marginStart="28dp"
               android:layout_marginLeft="28dp"
               android:layout_marginTop="12dp"
               android:layout_marginEnd="28dp"
               android:layout_marginRight="28dp"
               android:layout_marginBottom="12dp"
               android:background="@drawable/user_profile_1"
               app:layout_constraintBottom_toTopOf="@+id/guideline41"
               app:layout_constraintEnd_toStartOf="@+id/guideline10"
               app:layout_constraintHorizontal_bias="0.0"
               app:layout_constraintStart_toEndOf="@+id/guideline_UserProfile3"
               app:layout_constraintTop_toBottomOf="@+id/guideline11" />

           <TextView
               android:id="@+id/textView7"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_below="@+id/btnUserProfile"
               android:layout_centerHorizontal="true"
               android:layout_marginTop="10dp"
               android:text="USER PROFILE"
               android:textSize="14sp"
               app:layout_constraintEnd_toEndOf="@+id/btnUserProfile"
               app:layout_constraintStart_toStartOf="@+id/btnUserProfile"
               app:layout_constraintTop_toBottomOf="@+id/guideline41" />


      </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>